page {
    background-color: #FFF;
    position: relative;
}

.wrap {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 212rpx;
}

.wrap .label {
    position: relative;
    font-size: 36rpx;
    color: #00C600;
    letter-spacing: 0;
    text-align: center;
    line-height: 54rpx;
    font-weight: bold;
}

.wrap .label::before {
    content: '';
    position: absolute;
    width: 60rpx;
    height: 2rpx;
    top: 50%;
    left: -90rpx;
    background-color: #E6E6E6;
    z-index: 1;
}

.wrap .label::after {
    content: '';
    position: absolute;
    width: 60rpx;
    height: 2rpx;
    top: 50%;
    right: -90rpx;
    background-color: #E6E6E6;
    z-index: 1;
}

.wrap .price {
    font-size: 84rpx;
    color: #222222;
    letter-spacing: 0;
    text-align: center;
    line-height: 126rpx;
    font-weight: bold;
    margin-top: 8rpx;
}