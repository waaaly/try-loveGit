.integral-score {
    position: relative;
    width: 100%;
    height: 333rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: left top;
    color: #fff;
}

.integral-score .integral-rule {
    position: absolute;
    right: 0;
    top: 33rpx;
    z-index: 2;
}

.integral-score .integral-rule image {
    width: 125rpx;
    height: 50rpx;
}

.integral-score .score-main {
    position: absolute;
    width: 100%;
    height: 100%;
}

.integral-score .score-area {
    text-align: center;
}

.integral-score .score-area .score {
    font-size: 21rpx;
}

.integral-score .score-area .score text:first-child {
    font-size: 83rpx;
    font-weight: bold;
}

.integral-score .score-area .money {
    font-size: 27rpx;
    margin-top: 9rpx;
}

.integral-score .account-area .account-icon {
    text-align: center;
}

.integral-score .account-area .account-icon image {
    width: 62rpx;
    height: 62rpx;
}

.integral-score .account-area .account-describe {
    margin-top: 25rpx;
    font-size: 27rpx;
    color: #fff;
}

.integral-score .integral-deadline {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 52rpx;
    line-height: 54rpx;
    padding: 0 34rpx;
    background-image: url(data:image/png;base64,);
    background-size: 29rpx 29rpx;
    background-repeat: no-repeat;
    background-position: left 34rpx center;
    font-size: 23rpx;
    text-indent: 37rpx;
    color: rgba(255,255,255,0.6);
}

.integral-empty {
    padding-top: 167rpx;
    text-align: center;
}

.integral-empty .empty-icon image {
    width: 208rpx;
    height: 208rpx;
}

.integral-empty .empty-title {
    font-size: 32rpx;
    color: #888;
    margin-top: 8rpx;
    margin-bottom: 50rpx;
}

.integral-empty .empty-btn {
    height: 62rpx;
    line-height: 62rpx;
    padding: 0 35rpx;
    font-size: 27rpx;
    border-radius: 32rpx;
    border: 1rpx solid #333;
}

.integral-list {
    padding: 34rpx 0;
    margin: 0 34rpx;
    background-position: left bottom;
    background-image: linear-gradient(0deg,transparent 50%,#eaeaea 50%);
    background-repeat: repeat-x;
    background-size: 100% 1px;
}

.integral-list:last-child {
    background-image: none;
}

.integral-list .list-title {
    font-size: 29rpx;
}

.integral-list .list-score {
    font-size: 37rpx;
    margin-left: 17rpx;
}

.integral-list .list-score .green {
    color: #24c942;
}

.integral-list .list-time {
    font-size: 23rpx;
    color: #999;
    margin-top: 17rpx;
}

.integral-list .list-time text:first-child {
    margin-right: 12rpx;
}

.integral-list .list-info {
    font-size: 23rpx;
    color: #999;
    margin-top: 8rpx;
    align-items: baseline;
}

.integral-list .list-info .max-hg {
    height: 31rpx;
    overflow: hidden;
}

.integral-list .list-info .flex-shrink-1 {
    line-height: 1.5;
    padding-right: 8rpx;
}

.integral-list .list-info .list-icon {
    padding-top: 7rpx;
}

.integral-list .list-info .list-icon image {
    width: 21rpx;
    height: 21rpx;
}

.integral-list .list-info .list-icon.list-icon-down image {
    transform: rotate(90deg);
}

.integral-list .list-info .list-icon.list-icon-up image {
    transform: rotate(-90deg);
}