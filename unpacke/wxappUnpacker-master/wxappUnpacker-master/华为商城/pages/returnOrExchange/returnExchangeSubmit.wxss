.rma-form {
    margin-top: 25rpx;
}

.rmaSubmit-form {
}

.rmaSubmit-form .bg_white {
    margin: 21rpx 0 0 0;
    padding: 0 33rpx;
}

.rmaSubmit-form .rmaSubmit-form-title {
    padding: 35rpx 0 15rpx;
    font-size: 31rpx;
    font-weight: bold;
}

.rma-form .list {
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    font-size: 27rpx;
    border-bottom: 1px solid #eeeeee;
    position: relative;
}

.rma-form .form-reason .list {
    border-bottom: 0;
}

.rma-form .list .list-l {
    position: relative;
    width: 134rpx;
}

.rma-form .rma-submit-form-address .list {
    padding: 23rpx 0 6rpx;
    height: auto;
    line-height: 1;
}

.rma-form .list .list-l .state {
    position: relative;
    top: 3rpx;
}

.rma-form .list .lsit-r {
    box-flex: 1;
    display: block;
    flex: 1;
    position: relative;
    color: #cccccc;
}

.rma-form .list .lsit-r .select {
    padding: 0 50rpx 0 0;
    height: 83rpx;
    position: relative;
    text-align: right;
}

.rma-form .list .lsit-r .select .icon-arrows-more {
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -18rpx;
    display: inline-block;
    width: 37rpx;
    height: 37rpx;
    background-size: 100% auto;
    background-position: center center;
    background-image: url(data:image/png;base64,);
}

.rma-form .list .lsit-r .option {
    line-height: 87rpx;
    color: #333;
    font-size: 27rpx;
    font-weight: bold;
}

.rma-submit-form-reason .myRMAOrderList {
    display: block;
}

.list .sys-good-item {
    border-bottom: 0;
}

.list .rma-good-area .good-panels {
    padding-left: 58rpx;
}

.list .rma-good-area .good-panels .system-l {
    position: absolute;
    left: 0px;
    top: 50%;
    margin-top: -47rpx;
}

.list .rma-good-area .good-panels .good-icon {
    left: 50rpx;
    top: -8rpx;
}

.list .rma-good-area .good-panels .good-img {
    margin: 0 21rpx 42rpx 0;
    padding: 0;
}

.sys-good-item .p-subtotal {
    width: 180rpx;
    height: 48rpx;
}

.sys-good-item .p-amount-rma {
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    border: 2rpx solid #eeeeee;
    border-radius: 25rpx;
    overflow: hidden;
    margin-top: 7rpx;
}

.sys-good-item .p-amount-rma .total {
    width: 58rpx;
    height: 48rpx;
    text-align: center;
    font-size: 25rpx;
    line-height: 48rpx;
    border-left: 2rpx solid #eeeeee;
}

.sys-good-item .p-amount-rma.rma-opacity {
    opacity: 0.6;
}

.sys-good-item .p-amount-rma .Subtract,.sys-good-item .p-amount-rma .plus {
    display: inline-block;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 17rpx 17rpx;
}

.sys-good-item .p-amount-rma .Subtract {
    background-image: url(data:image/png;base64,);
}

.sys-good-item .p-amount-rma .plus {
    background-image: url(data:image/png;base64,);
}

.sys-good-item .p-amount-rma .total-input {
    width: 63rpx;
    height: 48rpx;
    line-height: 54rpx;
}

.sys-good-item .p-amount-rma .total.disabled {
    background-color: #eaeaea;
}

.sys-good-item .p-amount-rma .total:first-child {
    border: none;
}

.myRMAOrderList .good-panels .good-price-num {
    display: block;
    color: #3a3a3a;
}

.myRMAOrderList .good-panels .good-price-dark {
    color: #333;
}

.myRMAOrderList .good-panels .good-price-num.good-price-only {
    margin-right: 60rpx;
    width: 57rpx;
    text-align: center;
    margin-top: 15rpx;
}

.rma-form .uploadproblem {
    align-items: stretch;
    padding: 16rpx 0 35rpx;
}

.rma-form .uploadproblem .list-l {
    min-width: 110rpx;
}

.rma-form .uploadproblem .list-l .star {
    top: -11rpx;
}

.rma-form .uploadproblem .list-l .state {
    top: -16rpx;
}

.rma-form .uploadproblem .lsit-r textarea {
    width: 642rpx;
    height: 200rpx;
    line-height: 1.5;
    color: #333;
    margin-top: 5rpx;
    background: #fbfbfb;
    padding: 17rpx 21rpx;
    border-top-left-radius: 17rpx;
    border-top-right-radius: 17rpx;
}

.rma-form .uploadproblem .lsit-r .textarea-modal {
    display: block;
    width: 642rpx;
    height: 200rpx;
    line-height: 1.5;
    color: #333;
    background: #fbfbfb;
    padding: 17rpx 21rpx;
    border-top-left-radius: 17rpx;
    border-top-right-radius: 17rpx;
    margin-top: 5rpx;
}

.rma-form .uploadproblem .lsit-r .textarea-modal text {
    display: block;
    box-sizing: border-box;
    overflow: auto;
    height: 100%;
    width: 100%;
    padding: 0;
    margin: 0;
    margin-bottom: 40rpx;
}

.rma-form .uploadproblem .lsit-r .upload-limit {
    width: 642rpx;
    padding: 0 21rpx 10rpx;
    font-size: 23rpx;
    color: #999;
    margin-bottom: 25rpx;
    background: #fbfbfb;
    text-align: right;
    border-bottom-left-radius: 17rpx;
    border-bottom-right-radius: 17rpx;
}

.rma-form .uploadproblem .upload-limit .current {
    color: #333;
}

.rma-form .uploadproblem .lsit-r textarea.textareaIOS {
    margin-top: -7rpx;
}

.rma-form .uploadproblem .lsit-r .textarea-modal.textareaIOS {
    margin-top: -7rpx;
}

.rma-form .uploadproblem .lsit-r .textarea-modal.textareaIOS text {
    padding: 12rpx 10rpx;
    font-size: 27rpx;
}

.rma-form .uploadproblem .lsit-r input {
    padding: 0;
    height: 55px;
    padding-left: 15px;
}

.rma-submit-form-reason .uploadImg {
    padding: 16rpx 0;
    display: block;
    height: auto;
}

.rma-submit-form-reason .uploadImg .up-title {
    font-size: 27rpx;
    line-height: 2.7;
}

.rma-submit-form-reason .uploadImg .up-list {
    margin-right: 14rpx;
    margin-bottom: 14rpx;
    float: left;
    position: relative;
    background-color: #eaeaea;
    height: 124rpx;
    width: 124rpx;
    display: box;
    box-align: center;
    box-pack: center;
}

.rma-submit-form-reason .icon-img-del {
    position: absolute;
    right: 2rpx;
    bottom: 2rpx;
}

.rma-submit-form-reason .up-list .up-p {
    color: #ccc;
    font-size: 18rpx;
    text-align: center;
    position: absolute;
    bottom: 20rpx;
    left: 0;
    width: 100%;
}

.uploadImg .up-list input[type=file] {
    height: 100%;
    width: 100%;
    opacity: 0;
    position: absolute;
    top: 0;
    left: 0;
}

.uploadImg .up-tips {
    font-size: 20rpx;
    color: #888;
    line-height: 2.7;
}

.rmaSubmit-form .rma-submit-form-address {
    padding: 0 33rpx 40rpx;
}

.rma-submit-form-address .lsit-r {
}

.rma-submit-form-address .lsit-r input {
    margin-top: 2rpx;
    padding-right: 50rpx;
    color: #333;
}

.rma-submit-form-address .lsit-r .address-Select {
    color: #333;
}

.rma-submit-form-address .icon-arrows-down {
    position: absolute;
    right: 0;
    top: 10rpx;
}

.rma-form .return-noborder .list {
    border: 0;
}

.rma-tips {
    padding: 35rpx 0 0 33rpx;
    font-size: 31rpx;
}

.rma-tips .tips {
    padding: 0;
    background-color: #eee;
    font-size: 23rpx;
    color: #999;
}

.rma-tips .tips .h .tips-title {
    padding-bottom: 12rpx;
    color: #666;
    font-size: 27rpx;
}

.rma-tips .tips .strong {
    color: #666;
}

.rma-tips .explain view {
    margin-bottom: 11rpx;
}

.button-style-1-big,.button-style-1-big-disabled {
    height: 77rpx;
    line-height: 77rpx;
    font-size: 25rpx;
    background: linear-gradient(left,#E43E2D,#CA141D);
    box-shadow: none;
    border: 0;
    position: relative;
}

.rma-change-top {
}

.rma-change-top .rma-change-title {
    font-size: 25rpx;
    color: #666;
    padding: 31rpx 33rpx 23rpx;
}

.rma-change-top .ram-change-btn {
    padding: 0 33rpx 42rpx;
}

.rma-change-top .ram-change-btn view {
    width: 187rpx;
    height: 73rpx;
    line-height: 73rpx;
    margin-right: 23rpx;
    font-size: 31rpx;
    text-align: center;
    display: inline-block;
}

.rma-change-top .ram-change-btn .style-1 {
    border-radius: 5rpx;
    border: 3rpx solid rgba(51,51,51,0.3);
}

.rma-change-top .ram-change-btn .style-2 {
    border-radius: 5rpx;
    border: 3rpx solid #CA151E;
    color: #CA151E;
}

.rma-change-goods {
    margin-top: 21rpx;
    padding: 0 33rpx;
}

.rma-change-goods .change-good-title {
    padding: 37rpx 0;
    font-size: 31rpx;
    font-weight: bold;
}

.rma-change-goods .change-good-error {
    width: 450rpx;
    margin: 5rpx auto 0;
    padding-bottom: 62rpx;
    text-align: center;
    font-size: 25rpx;
    color: #999999;
}

.rma-change-goods .change-good-error view:first-child {
    margin-bottom: 8rpx;
}

.rma-change-goods .change-good-error.hide {
    display: none;
}

.checkbox checkbox+.state {
    background-size: 100%;
    width: 39rpx;
    height: 39rpx;
    background-image: url(data:image/png;base64,);
}

.checkbox .disabled+.state {
    background-size: 100%;
    width: 39rpx;
    height: 39rpx;
    background-image: url(data:image/png;base64,);
}

.rma-change-goods .good-panels .good-img image {
    width: 146rpx;
    height: 146rpx;
}

.rma-change-goods .rma-good-item .good-panels .good-name {
    width: 446rpx;
    margin: 11rpx 0 0 0;
    font-size: 25rpx;
    line-height: 1.4;
}

.rma-change-goods .good-info .good-label {
    position: relative;
    display: inline-block;
    min-width: 65rpx;
    height: 31rpx;
    line-height: 1.45;
    padding: 0 3rpx;
    margin-right: 11rpx;
    display: inline-block;
    border: 3rpx solid #ca141d;
    border-radius: 15rpx;
    color: #ca141d;
    text-align: center;
    font-size: 21rpx;
    vertical-align: middle;
    box-sizing: border-box;
}

.rma-change-goods .good-info .good-num-tips {
    font-size: 25rpx;
    color: #999;
    line-height: 38rpx;
    margin-top: 10rpx;
}

.rma-change-goods .good-info .good-num-tips .red {
    position: relative;
    padding-left: 28rpx;
}

.rma-change-goods .good-info .good-num-tips .red::before {
    content: "";
    position: absolute;
    left: 0;
    top: 9rpx;
    width: 21rpx;
    height: 21rpx;
    background: url(data:image/png;base64,);
    background-size: 100%;
}

.rma-good-item.rma-good-border {
    border-top: 3rpx solid #eaeaea;
    padding-top: 42rpx;
}

.rma-change-goods .rma-good-item .good-panels .good-num {
    padding-right: 0;
}

.rma-change-goods .rma-good-area {
    padding-top: 42rpx;
    border-bottom: 3rpx solid #eaeaea;
}

.rma-change-goods .rma-good-area:first-child {
    padding-top: 0;
}

.rma-change-goods .goods-num-total {
    height: 83rpx;
    line-height: 83rpx;
    font-size: 25rpx;
    text-align: right;
}

.rma-change-goods .goods-num-total text {
    font-weight: bold;
    margin: 0 5rpx;
}

.choose-status {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 100;
}

.choose-reason {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 80;
}

.btn-picker {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
}