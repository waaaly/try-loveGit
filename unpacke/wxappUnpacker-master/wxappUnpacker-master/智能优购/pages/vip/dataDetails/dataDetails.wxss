page {
    background: #fff;
}

.dat {
}

.dat_headers {
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background: #fff;
}

.dat_header {
    height: 150rpx;
    border-top: 1rpx solid #ddd;
    padding: 18rpx 0 28rpx 35rpx;
    font-size: 30rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: #333;
}

.dat_header_num {
    font-family: FZLTHJW--GB1-0;
    font-size: 100rpx;
    font-weight: 400;
}

.dat_content {
    padding-top: 214rpx;
}

.dat_content_header {
    padding: 0 23rpx;
    height: 66rpx;
    background-color: #f2f2f2;
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(102,102,102,1);
}

.dat_spa {
    margin-right: 20rpx;
}

.dat_item {
    padding: 33rpx 23rpx;
}

.dat_item:nth-child(2n+1) {
    background: #fff;
}

.dat_item:nth-child(2n) {
    background: #f2f2f2;
}

.dat_item_inx {
    width: 100rpx;
    font-size: 32rpx;
    padding-left: 20rpx;
    font-family: FZLTZCHK--GBK1-0;
    font-weight: bold;
    line-height: 100rpx;
    color: rgba(15,15,15,1);
}

.dat_item_name {
    width: 353rpx;
    font-size: 28rpx;
    line-height: 50rpx;
    font-family: FZLTZCHK--GBK1-0;
    font-weight: bold;
    margin-left: 33rpx;
    color: rgba(15,15,15,1);
}

.dat_phone {
    text-decoration: underline;
    color: #3C79C4;
    font-weight: normal;
}

.dat_item_img {
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
}

.dat_item_num {
    width: 140rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
}