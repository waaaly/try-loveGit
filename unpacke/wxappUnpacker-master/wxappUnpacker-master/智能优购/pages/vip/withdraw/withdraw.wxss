.iden {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    border-top: 1rpx solid #ddd;
    background: rgba(242,242,242,1);
}

.iden-box {
    width: 700rpx;
    padding-top: 20rpx;
    margin: 0 auto;
}

.phcolors {
    font-size: 30rpx;
    padding-top: 10rpx;
    color: #c4c4c4;
}

.iden-form {
    background-color: #fff;
    border-radius: 10rpx;
    padding: 0 25rpx 0;
}

.iden-item {
    height: 128rpx;
    line-height: 128rpx;
    border-bottom: 1rpx solid #ddd;
}

.iden-btn {
    margin-top: 40rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 28rpx;
    text-align: center;
    border-radius: 10rpx;
    color: #fff;
    background-color: #dfb87f;
}

.iden-item-tit {
    display: block;
    width: 115rpx;
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(102,102,102,1);
}

.iden-item-input {
    width: 500rpx;
    margin-left: 20rpx;
    color: #222;
    font-size: 26rpx;
}

.phcolor {
    color: #c4c4c4;
}

.iden-code-btn {
    width: 180rpx;
    padding: 0;
    background-color: transparent;
    text-align: center;
    color: #f11717;
    margin-left: 30rpx;
    font-size: 26rpx;
}

.iden-code-btn::after {
    border: 0;
}

.btn_hover {
    background-color: none;
}

button[disabled] {
    color: #f11717;
    background: transparent;
}

.iden_picker {
    width: 500rpx;
    height: 60rpx;
    margin-left: 60rpx;
}

.iden-item-inputs {
    width: 460rpx;
}

.withdraw {
    padding-top: 35rpx;
    border-bottom: 1rpx solid #ddd;
}

.withdraw_tit {
    padding-top: 35rpx;
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(102,102,102,1);
}

.withdraw_num {
    padding: 38rpx 0;
    align-items: flex-end;
    font-size: 30rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102,102,102,1);
}

.withdraw_nums {
    font-size: 60rpx;
    font-family: ArialMT;
    font-weight: 400;
    color: rgba(34,34,34,1);
}

.withdraw_all {
    line-height: 70rpx;
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(51,51,51,1);
}

.withdraw_all_btn {
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: #deb57e;
}

.withdraw_input {
    height: 60rpx;
    padding-left: 20rpx;
    line-height: 60rpx;
    font-size: 60rpx;
    font-family: ArialMT;
    font-weight: 400;
    color: rgba(34,34,34,1);
}

.iden_tip {
    font-size: 20rpx;
    text-align: center;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    margin-top: 20rpx;
    color: rgba(180,180,180,1);
}