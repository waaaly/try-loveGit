.withdrawR {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    border-top: 1rpx solid #ddd;
    background: rgba(242,242,242,1);
}

.withdrawR_headers {
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(242,242,242,1);
    z-index: 2;
    overflow: hidden;
}

.withdrawR_header {
    width: 710rpx;
    margin: 20rpx auto 10rpx;
    font-size: 26rpx;
    border-radius: 8rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    height: 130rpx;
    background-color: #fff;
    padding: 40rpx 30rpx;
    color: rgba(102,102,102,1);
}

.withdrawR_h_item {
    width: 353rpx;
    height: 100%;
}

.spa_font1 {
    font-size: 40rpx;
    font-family: HiraginoSansGB-W3;
    font-weight: normal;
    color: rgba(34,34,34,1);
}

.spa_font2 {
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: normal;
    color: rgba(102,102,102,1);
}

.spa_font3 {
    font-size: 26rpx;
    font-family: HiraginoSansGB-W3;
    font-weight: normal;
    margin-top: 5rpx;
    color: rgba(102,102,102,1);
}

.with_line {
    border-right: 1rpx solid #ddd;
    height: 54rpx;
    color: #e8e8e8;
}

.withdrawR_header_num {
    font-size: 40rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(34,34,34,1);
}

.withdrawR_content {
    margin-top: 145rpx;
}

.withdrawR_item {
    width: 710rpx;
    margin: 0 auto;
    height: 160rpx;
    background-color: #fff;
}

.withdrawR_item_in {
    height: 100%;
    width: 650rpx;
    margin: 0 auto;
    font-size: 26rpx;
}

.bor {
    border-bottom: 1rpx solid #E8E8E8;
}

.withdrawR_item_txt1 {
    font-size: 26rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    float: left;
    color: rgba(102,102,102,1);
}

.withdrawR_item_txt2 {
    font-size: 26rpx;
    margin-left: 20rpx;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(241,23,23,1);
}

.withdrawR_item_txt3 {
    font-size: 24rpx;
    float: right;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: rgba(215,178,133,1);
}

.withdrawR_item_txt4 {
    font-size: 26rpx;
    float: right;
    font-family: FZLTHJW--GB1-0;
    font-weight: 400;
    color: #222;
}

.withdrawR_item_txt5 {
    font-size: 26rpx;
    font-family: HiraginoSansGB-W3;
    font-weight: normal;
    color: rgba(241,23,23,1);
}

.withdrawR_item_time {
    font-size: 22rpx;
    margin-top: 15rpx;
    font-family: ArialMT;
    font-weight: 400;
    color: rgba(199,199,199,1);
}

.withdrawR_item_timesa {
    letter-spacing: 8rpx;
}

.withdrawR_item_timesb {
    margin-left: 20rpx;
}