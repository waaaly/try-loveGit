    border-radius: 8rpx;
}

.expand .button {
    border-top: 1px solid #f1f1f1;
}

.expand .button view {
    width: 50%;
    height: 90rpx;
    font-size: 14px;
    line-height: 90rpx;
    text-align: center;
}

.expand .button .confirm {
    color: #357C6E;
    position: relative;
}

.expand .button button {
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 90rpx;
    z-index: 80;
    opacity: 0;
}

.expand .button .cancel {
    float: right;
    border-left: 1px solid #f1f1f1;
