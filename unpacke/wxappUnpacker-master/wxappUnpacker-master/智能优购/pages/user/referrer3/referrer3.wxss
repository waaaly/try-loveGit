}

.expand .title {
    margin-left: 60rpx;
    text-align: left;
    font-size: 14px;
    height: 70rpx;
    line-height: 70rpx;
}

.expand input {
    width: 440rpx;
    font-size: 14px;
    height: 65rpx;
    border: 1px solid #f1f1f1;
    margin-left: 60rpx;
    padding: 0rpx 20rpx 0rpx 20rpx;
    border-radius: 3px;
}

.expand .prompt {
    font-size: 12px;
    height: 80rpx;
    line-height: 80rpx;
