page {
    background-color: #fff;
}

.partakeActive {
    padding: 0 34rpx;
}

.partakeActive-item {
    padding: 32rpx 0;
    border-bottom: 3rpx solid #e0e0e0;
}

.partakeActive-user-img {
    width: 125rpx;
    height: 125rpx;
    padding-right: 25rpx;
}

.partakeActive-user-img view {
    width: 100%;
    height: 100%;
    overflow: hidden;
    border-radius: 50%;
}

.partakeActive-user-img image {
    width: 125rpx;
    height: 125rpx;
}

.partakeActive-user .partakeActive-user-info {
    font-size: 28rpx;
    color: #666;
    line-height: 2.5;
}

.partakeActive-user .partakeActive-user-info .partakeActive-user-name {
    color: #333;
    padding-right: 11rpx;
}

.partakeActive-user .partakeActive-user-time {
    font-size: 25rpx;
    color: #999;
    line-height: 2.5;
}

.partakeActive-item .btn-next .icon-more-right,.partakeActive-item .btn-next image {
    width: 32rpx;
    height: 32rpx;
}