page {
    border-top: 0!important;
}

.userInfo {
    margin-bottom: 25rpx;
}

.userInfo .u-face {
    width: 100%;
    height: 200rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: center bottom;
    background-size: 100% auto;
    position: relative;
}

.userInfo .u-face .u-img {
    width: 134rpx;
    height: 134rpx;
    border-radius: 50%;
    background-color: #fff;
    padding: 7rpx;
    position: absolute;
    bottom: -23rpx;
    left: 50%;
    margin-left: -74rpx;
    overflow: hidden;
}

.userInfo .u-face .u-img image {
    width: 134rpx;
    height: 134rpx;
    border-radius: 50%;
    overflow: hidden;
}

.userInfo .u-face button {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 2;
}

.userInfo .u-name-active {
    text-align: center;
    padding: 36rpx 0 73rpx;
    position: relative;
}

.userInfo .u-name-active .u-name {
    font-size: 34rpx;
    line-height: 1.5;
    display: block;
    text-align: center;
}

.userInfo .u-name-active .u-id {
    font-size: 23rpx;
    color: #999;
    padding-bottom: 11rpx;
}

.userInfo .u-name-active .linkAccountBtn {
    margin-top: 5rpx;
    position: absolute;
    left: 0;
    right: 0;
}

.sm-header {
    height: 63rpx;
    padding: 0 34rpx;
}

.sm-header text {
    font-size: 25rpx;
    line-height: 63rpx;
    color: #888;
}

.sm-box {
    padding: 0 34rpx;
}

.sm-box .sm-img {
    padding-right: 21rpx;
}

.sm-box .sm-img image {
    width: 42rpx;
    height: 42rpx;
}

.sm-box .sm-list {
    height: 100rpx;
    border-bottom: 1px solid #eaeaea;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.sm-box .sm-list:last-child {
    border-bottom: 0;
}

.sm-box .sm-list .sm-title {
    font-size: 28rpx;
}

.sm-box .sm-list .btn-next {
    font-size: 23rpx;
}

.sign-out-area {
    height: 100rpx;
    margin-top: 46rpx;
}

.sign-out {
    font-size: 30rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    background-color: #fff;
}

.tips-pure .tips-cont {
    line-height: 1.3;
}

.has-padding {
    padding-top: 42rpx;
}