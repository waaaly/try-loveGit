.rma-process .rma-process-list .rma-process-node icon {
    width: 15rpx;
    height: 15rpx;
    border-radius: 50%;
    margin-left: 9rpx;
    margin-top: 11rpx;
    background-color: #ccc;
    display: block;
    position: relative;
    z-index: 10;
}

.rma-process .rma-process-list:first-child .rma-process-node icon {
    background-color: #ffb0b4;
}

.rma-process .rma-process-list .rma-process-cont {
    font-size: 23rpx;
    color: #888;
    width: 100%;
    -webkit-box-flex: 1;
    flex-shrink: 1;
    overflow: hidden;
}

.rma-process .rma-process-list:first-child .rma-process-cont .rma-process-text {
    line-height: 1.3;
    color: #ca141d;
}

.rma-process .rma-process-list .rma-process-cont .rma-process-time {
    padding-top: 5rpx;
}

.rma-process-footer {
    font-size: 23rpx;
    height: 88rpx;
