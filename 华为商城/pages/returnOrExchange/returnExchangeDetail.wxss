    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.rma-process-footer .rma-order-time {
    color: #ccc;
}

.rma-process-footer navigator {
