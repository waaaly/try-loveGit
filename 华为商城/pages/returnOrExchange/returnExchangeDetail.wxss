.rma-form {
    margin-bottom: 21rpx;
}

.rma-progress {
    height: 209rpx;
    padding: 0 34rpx;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
}

.rma-progress-tab {
    flex: 1;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    text-align: center;
    position: relative;
}

.rma-progress-tab:after {
    content: '';
    width: auto;
    height: 5rpx;
    border-radius: 10rpx;
    background-color: #24c942;
    position: absolute;
    left: 50%;
    right: -50%;
    top: 17rpx;
    margin-left: 25rpx;
    margin-right: 25rpx;
}

.rma-progress-tab.current:after,.rma-progress-tab.current~.rma-progress-tab:after {
    content: '';
    background-color: #ccc;
}

.rma-progress-tab:last-child:after {
    height: 0;
}

.rma-progress-tab icon {
    width: 38rpx;
    height: 38rpx;
    margin: 0 auto;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: center;
    background-size: 100% 100%;
}

.rma-progress-tab.current icon {
    background-image: url(data:image/png;base64,);
}

.rma-progress-tab.current~.rma-progress-tab icon {
    background-image: url(data:image/png;base64,);
}

.rma-progress-tab text {
    font-size: 25rpx;
    color: #24c942;
    line-height: 1;
    display: block;
    margin-top: 25rpx;
}

.rma-progress-tab.current~.rma-progress-tab text {
    color: #ccc;
}

.rma-header {
    height: 84rpx;
    border-bottom: 3rpx solid #eee;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.rma-header .rma-header-txt {
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
}

.rma-header .rma-header-txt image {
    width: 32rpx;
    height: 32rpx;
}

.rma-header .rma-header-txt text {
    font-size: 30rpx;
    color: #333;
    display: block;
    margin-left: 13rpx;
}

.rma-header .rma-header-a {
    font-size: 25rpx;
    color: #267eff;
}

.arrow-more {
    width: 32rpx;
    height: 32rpx;
}

.arrow-more image {
    width: 32rpx;
    height: 32rpx;
}

.arrow-more-top image {
    transform: rotate(-90deg);
}

.arrow-more-down image {
    transform: rotate(90deg);
}

.rma-explain {
    padding: 0 34rpx;
}

.rma-process {
    padding: 28rpx 0;
}

.rma-process .rma-process-list {
    padding-bottom: 28rpx;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    position: relative;
}

.rma-process.rma-process-down .rma-process-list {
    display: none;
}

.rma-process .rma-process-list:last-child,.rma-process.rma-process-down .rma-process-list:first-child {
    padding-bottom: 0;
}

.rma-process.rma-process-down .rma-process-list:first-child {
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
}

.rma-process .rma-process-list:after {
    content: '';
    width: 3rpx;
    height: 100%;
    background-color: #ccc;
    position: absolute;
    left: 15rpx;
    top: 19rpx;
    z-index: 5;
}

.rma-process .rma-process-list:first-child:after {
    content: '';
    background-color: #ffb0b4;
}

.rma-process.rma-process-down .rma-process-list:first-child:after {
    content: '';
    background-color: #fff;
}

.rma-process .rma-process-list:last-child:after {
    content: '';
    background-color: #fff;
}

.rma-process .rma-process-list .rma-process-node {
