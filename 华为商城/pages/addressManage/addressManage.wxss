.address-manage-box {
    margin-bottom: 13rpx;
    padding: 12rpx 33rpx 17rpx;
    background: #fff;
}

.address-manage-box:first-child {
    border-top: 0;
}

.address-manage-box .user-info {
    font-size: 31rpx;
}

.address-manage-box .user-info .tel {
    margin-left: 10rpx;
}

.address-manage-box .address-info {
    position: relative;
    margin-top: 8rpx;
    font-size: 23rpx;
    color: #bbb;
    margin-bottom: 12rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.address-manage-box .address-btn {
    font-size: 23rpx;
    color: #888;
    padding-top: 17rpx;
    border-top: 1rpx solid #eaeaea;
}

.address-manage-box .address-btn .checkbox-box {
    margin-right: 17rpx;
}

.address-manage-box .address-btn .btn-all {
    position: relative;
    padding-left: 50rpx;
    margin-left: 33rpx;
}

.address-manage-box .address-btn .btn-all:before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 37rpx;
    height: 37rpx;
    background-size: 100%;
}

.address-manage-box .address-btn .btn-all.delate-btn:before {
    background-image: url(data:image/png;base64,);
}

.address-manage-box .address-btn .btn-all.update-btn:before {
    background-image: url(data:image/png;base64,);
}

.button-area-3 .button-flex view {
    display: inline-block;
    position: relative;
}

.button-area-3 .button-flex icon {
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    left: -44rpx;
    top: 50%;
    margin-top: -16rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: center;
    background-size: 100%;
}