page {
    background-color: #fff;
}

.upLine {
    height: 17rpx;
    background-color: rgba(0,0,0,0.05);
}

.registerBox {
    padding: 0 34rpx;
    margin: 34rpx 0;
}

.registerBox .upItem {
    margin-bottom: 34rpx;
}

.registerBox .upItem .eyeon,.registerBox .upItem .eyeoff {
    height: 100%;
    padding-left: 17rpx;
    -webkit-box-flex: 0;
    flex-shrink: 0;
    overflow: hidden;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
}

.registerBox .upItem .eyeon icon,.registerBox .upItem .eyeoff icon {
    width: 50rpx;
    height: 50rpx;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 100% 100%;
}

.registerBox .upItem .eyeon icon {
    background-image: url(data:image/png;base64,);
}

.registerBox .upItem .eyeoff icon {
    background-image: url(data:image/png;base64,);
}

.registerBox .upItem .system-l-l-r {
    font-size: 28rpx;
    margin-bottom: 34rpx;
}

.registerBox .upItem .system-l-l-r .checkbox checkbox+.state {
    background-image: url(data:image/png;base64,);
}

.registerBox .upItem .system-l-l-r .system-between-l {
    padding-left: 13rpx;
}

.registerBox .upCondition {
    font-size: 28rpx;
    line-height: 1.5;
    color: rgba(0,0,0,0.5);
    padding-top: 17rpx;
}

.upOther {
    padding: 34rpx;
    margin-bottom: 50rpx;
}

.upOther .upToPhone {
    font-size: 32rpx;
    color: #007dff;
    padding-bottom: 9rpx;
}

.upOther .upToPhone:active {
    color: #0064cc;
}

.upOtherMargin {
    margin-bottom: 0;
}

.upOtherMargin .upOtherTitle {
    font-size: 32rpx;
    color: #000;
    margin-bottom: 7rpx;
}

.upOtherMargin .upOtherTip {
    font-size: 28rpx;
    color: rgba(0,0,0,0.5);
}

.upActive {
    padding: 0 34rpx;
    margin: 34rpx 0;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
}