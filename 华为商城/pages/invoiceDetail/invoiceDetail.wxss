.invoice-type {
    margin-top: .3rem;
}

.invoice-type view {
    font-size: 25rpx;
    line-height: 2.9;
    font-weight: bold;
    color: #333;
    padding: 0 34rpx;
    background-color: #fff;
    border-bottom: 1px solid #eee;
}

.invoice-form-area {
    padding: 0 34rpx;
}

.invoice-form-row {
    border-bottom: 1px solid #eee;
    padding: 25rpx 0;
}

.invoice-form-row .o-subject {
    font-size: 30rpx;
    padding-bottom: 15rpx;
}

.invoice-form-row .o-subhead {
    font-size: 23rpx;
    color: #888;
    padding-top: 9rpx;
    word-break: break-all;
}

.invoice-track {
    margin: 23rpx 0;
    position: relative;
    overflow: hidden;
}

.invoice-track:before {
    content: '';
    width: 3rpx;
    height: 100%;
    background-color: #c6c6c6;
    position: absolute;
    left: 163rpx;
    top: 11rpx;
}

.invoice-track .track-item {
    align-items: flex-start;
    position: relative;
}

.invoice-track .track-item:before {
    content: '';
    width: 28rpx;
    height: 28rpx;
    position: absolute;
    left: 150rpx;
    top: 8rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: left top;
    background-size: 28rpx auto;
    z-index: 5;
}

.invoice-track .track-item:first-child:before {
    content: '';
    background-position: left bottom;
}

.invoice-track .track-item .track-process {
    font-size: 19rpx;
    color: #888;
    line-height: 1.5;
    text-align: right;
    width: 134rpx;
    padding-right: 31rpx;
    padding-top: 8rpx;
}

.invoice-track .track-item .track-process text {
    font-weight: bold;
}

.invoice-track .track-item .track-process-cont {
    font-size: 30rpx;
    line-height: 1.5;
    color: #888;
    padding: 0 0 40rpx 34rpx;
    position: relative;
}

.invoice-track .track-item:first-child .track-process-cont {
    color: #ca151d;
}

.invoice-track .track-item:last-child .track-process-cont:before {
    content: '';
    width: 3rpx;
    height: 100%;
    background-color: #fff;
    position: absolute;
    left: 0;
    top: 11rpx;
}