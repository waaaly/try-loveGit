@import "../../app.wxss";

.address-loading {
    font-size: 27rpx;
}

.address-loading .loading-left {
    color: #999999;
}

.address-loading .loading-left .address-empty {
    padding-left: 50rpx;
    font-size: 31rpx;
    color: #333;
    background-image: url(data:image/png;base64,);
    background-position: left center;
    background-size: 38rpx 38rpx;
    background-repeat: no-repeat;
}

.address-loading .loading-left text {
    color: #267eff;
}

.address-loading .loading-right {
    color: #666;
}

.address-loading .loading-right image {
    width: 26rpx;
    height: 26rpx;
    margin-left: 17rpx;
    vertical-align: middle;
}

.address-pop {
    height: 80%;
}

.address-pop .button-style-4 {
    box-shadow: none;
    border: 0;
    line-height: 83rpx;
}

.address-pop .button-style-4 text {
    font-size: 31rpx;
}

.address-pop .ecWap-box-ok image {
    width: 50rpx;
    height: 50rpx;
    vertical-align: middle;
}

.address-pop .ecWap-box-ok text {
    vertical-align: middle;
}