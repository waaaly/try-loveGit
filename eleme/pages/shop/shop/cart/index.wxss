.cart {
    position: relative;
    width: 100%;
    font-size: 32rpx;
}

.cart-header,.cart-body {
    padding: 0 30rpx;
}

.cart-header {
    border-bottom: 1rpx solid #ddd;
    background-color: #eceff1;
    color: #666;
    height: 80rpx;
    line-height: 32rpx;
    padding-right: 10rpx;
}

.cart-header-text {
    padding-left: 10rpx;
    font-weight: bold;
    border-left: 6rpx solid #0097FF;
    color: rgb(102,102,102);
}

.cart-empty {
    font-size: 26rpx;
    color: rgb(102,102,102);
    line-height: 26rpx;
    padding: 10rpx;
    padding-right: 20rpx;
}

.cart-empty-icon {
    margin-right: 6rpx;
    width: 26rpx;
    height: 26rpx;
    position: relative;
    top: 4rpx;
}

.is-empty {
    font-size: 24rpx;
    opacity: .5;
}

.is-zero-agent {
    opacity: .5;
}

.cart-body {
    background: #fff;
    line-height: 32rpx;
    max-height: 666rpx;
    overflow-y: auto;
}

.piecewise-agent-fee {
    font-size: 20rpx;
    padding: 15rpx 0;
    font-size: 22rpx;
    border-bottom: 1rpx solid #eee;
    color: rgb(102,102,102);
}

.piecewise-agent-fee-label {
    display: inline-flex;
    align-items: center;
    padding: 1rpx 3rpx 0;
    font-size: 22rpx;
    height: 24rpx;
    margin-right: 10rpx;
    color: #ff6000;
    font-weight: bold;
    border: 1rpx solid #ff6000;
    border-radius: 4rpx;
}

.cart-item {
    width: 100%;
    padding: 32rpx 0;
    border-bottom: 1rpx solid #eee;
}

.cart-item-spec {
    color: rgb(102,102,102);
    font-size: 24rpx;
}

.item-total-price {
    color: rgb(255,96,0);
    text-align: center;
    font-size: 20rpx;
}

.item-total-price-label {
    position: relative;
    top: 4rpx;
}

.item-total-discount-price-value {
    font-size: 32rpx;
}

.item-total-original-price-value {
    font-size: 22rpx;
    margin-right: 5px;
    color: #999;
    text-decoration: line-through;
}

.cart-item-ctrl {
    width: 44rpx;
    height: 44rpx;
    padding: 10rpx;
}

.cart-footer {
    display: flex;
    flex-direction: row;
    height: 96rpx;
    width: 100%;
    color: #fff;
    z-index: 2;
    position: relative;
}

.cart-left {
    background: rgb(61,61,63);
    width: calc(100% - 210rpx);
    height: 100%;
    opacity: .95;
}

.cart-icon {
    height: 94rpx;
    width: 94rpx;
    border-radius: 50%;
    background: #0097FF;
    color: #fff;
    position: absolute;
    bottom: 14rpx;
    left: 35rpx;
}

.empty-cart-icon {
    background: rgb(51,51,51);
}

.empty-cart-icon::before {
    opacity: .3;
}

.kaTip {
    position: absolute;
    display: flex;
    justify-content: center;
    top: -64rpx;
    color: #333;
    line-height: 64rpx;
    height: 64rpx;
    width: 100%;
    font-size: 26rpx;
    text-align: center;
    background-color: #fffad6;
    box-shadow: 0 -1px 0 0 #eeeeee;
}

.kaTipClose {
    position: absolute;
    right: 30rpx;
    top: 20rpx;
    width: 24rpx;
    height: 24rpx;
}

.kaTipLink {
    color: #3190e8;
}

.cart-icon::before {
    content: '';
    width: 94rpx;
    height: 94rpx;
    background-size: cover;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
}

.cart-icon-bg {
    width: 122rpx;
    height: 122rpx;
    position: absolute;
    top: -29rpx;
    left: 20rpx;
    z-index: -5;
}

.cart-item-total-amount {
    background: rgb(255,70,29);
    color: #fff;
    font-size: 20rpx;
    line-height: 18rpx;
    border-radius: 12rpx;
    padding: 4rpx 10rpx 4rpx 10rpx;
    position: absolute;
    top: -22rpx;
    left: 112rpx;
}

.cart-cost {
    flex-direction: column;
    justify-content: center;
    position: absolute;
    height: 96rpx;
    width: 380rpx;
    left: 160rpx;
}

.left {
    left: 30rpx;
}

.cart-final-cost {
    font-size: 40rpx;
    color: #fff;
    height: 44rpx;
}

.cart-original-total {
    margin-left: 10rpx;
    color: #999;
    font-size: 30rpx;
    text-decoration: line-through;
}

.cart-delivery-text {
    font-size: 22rpx;
    color: #fff;
}

.cart-original-cost::after {
    content: "";
    position: absolute;
    border-top: 1rpx solid rgb(102,102,102);
    right: 0;
    top: 40%;
    left: 0;
}

.cart-submit-btn {
    font-size: 30rpx;
    position: absolute;
    right: 0;
    width: 210rpx;
    height: 100%;
    background: rgb(83,83,86);
}

.btn-abled {
    background: rgb(56,202,115);
}

.cart-mask {
    position: fixed;
    top: 0;
    left: 0;
    height: calc(100% + 360rpx);
    width: 100%;
    background: #000;
    opacity: .4;
    z-index: 1;
    transform: translateY(-360rpx);
    transition: opacity .2s ease;
    display: block;
}

.cart-mask-hidden {
    transition: opacity .2s ease;
    opacity: 0;
    display: none;
}

.cart-main {
    width: 100%;
    position: absolute;
    transition: all .3s ease;
    transform: translateY(-100%);
    opacity: 1;
    z-index: 2;
}

.cart-main-folded {
    transition: all .3s ease;
    transform: translateY(0);
    opacity: 0;
}

.essential-tip {
    position: absolute;
    bottom: 150rpx;
    right: 10rpx;
    color: #fff;
    padding: 10rpx 20rpx;
    box-shadow: 0rpx 1rpx 2rpx #ddd;
    font-size: 24rpx;
    font-weight: bold;
    background-color: #ff6000;
    outline: none;
    border: 6rpx solid #fff;
    border-radius: 50rpx;
    z-index: 10;
}

.btn-sm-text {
    font-size: 22rpx;
}